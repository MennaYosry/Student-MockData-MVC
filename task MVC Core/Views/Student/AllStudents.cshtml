@{
}
<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Name</th>
            <th scope="col">Address</th>
            <th scope="col">Img</th>
            <th scope="col">Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var student in Model)
        {
            <tr>
                <td>@student.Id</td>
                <td>@student.Name</td>
                <td>@student.Address</td>
                <td>
                    <img width="300px" height="300px" src="/students/@student.Img"/>
                </td>
                <td>
                    <a href="/student/studentByid/@student.Id">
                        Profile
                    </a>
                </td>
            </tr>
        }
      
    </tbody>
</table>
